{
  "name": "server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "gen-env-types": "gen-env-types .env -o src/common/types/env.d.ts -e ."
  },
  "dependencies": {
    "@mikro-orm/core": "^5.1.1",
    "@mikro-orm/migrations": "^5.1.1",
    "@mikro-orm/nestjs": "^4.3.1",
    "@mikro-orm/postgresql": "^5.1.1",
    "@nestjs/common": "^8.3.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^8.4.3",
    "@nestjs/platform-socket.io": "^8.3.1",
    "@nestjs/swagger": "^5.2.0",
    "@nestjs/websockets": "^8.3.1",
    "argon2": "^0.28.5",
    "buffer-to-stream": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cloudinary": "^1.28.1",
    "dotenv": "^16.0.0",
    "gen-env-types": "^1.3.2",
    "html-escaper": "^3.0.3",
    "jsonwebtoken": "^8.5.1",
    "nanoid": "^3.3.1",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "socket.io": "^4.4.1",
    "swagger-ui-express": "^4.3.0"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^5.0.2",
    "@nestjs/cli": "^8.2.4",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/buffer-to-stream": "^1.0.0",
    "@types/express": "^4.17.13",
    "@types/html-escaper": "^3.0.0",
    "@types/multer": "^1.4.7",
    "@types/node": "^17.0.23",
    "prettier": "^2.3.2",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.3.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}
